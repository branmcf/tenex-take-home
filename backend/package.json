{
    "name": "hardwire-api",
    "version": "0.1.0",
    "type": "module",
    "description": "A Node.js + Express API",
    "main": "index.js",
    "scripts": {
        "test:int": "jest --config jest/integrationTests.config.json",
        "test:int-debug": "myEnv=staging CLEAR_TEST_DATA=true SKIP_REDIS_CACHE=true IS_INTEGRATION_TESTING=true node --inspect-brk node_modules/.bin/jest --maxWorkers=3 --config jest/integrationTests.config.json",
        "test:unit": "jest --config jest/unitTests.config.json",
        "test:evals": "jest --config jest/evals.config.json",
        "test:evals-live": "RUN_LIVE_EVALS=true jest --config jest/evals.config.json",
        "dev": "tsx watch --env-file=.env index.ts",
        "build": "rm -rf dist/ ; tsc && cp -R public dist/public",
        "start": "node scripts/migrate-and-start.js",
        "gc-compile": "graphql-codegen --config codegen.ts",
        "gc-watch": "graphql-codegen --config codegen.ts -w",
        "migrate:create": "node-pg-migrate create --template-file-name ./scripts/migration-template.ts --tsconfig ./tsconfig.json",
        "migrate:up": "node-pg-migrate up --tsconfig ./tsconfig.json",
        "migrate:down": "node-pg-migrate down --tsconfig ./tsconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/branmcf/tenex-take-home.git"
    },
    "author": "Brandon McFarland",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/branmcf/tenex-take-home/issues"
    },
    "homepage": "https://github.com/branmcf/tenex-take-home/backend#readme",
    "devDependencies": {
        "@eslint/js": "^9.20.0",
        "@graphql-codegen/add": "^5.0.3",
        "@graphql-codegen/cli": "^5.0.5",
        "@graphql-codegen/client-preset": "^4.6.4",
        "@graphql-codegen/near-operation-file-preset": "^3.0.0",
        "@graphql-codegen/typescript": "^4.1.5",
        "@graphql-codegen/typescript-operations": "^4.5.1",
        "@jest/test-sequencer": "^29.7.0",
        "@stylistic/eslint-plugin": "^3.1.0",
        "@types/cls-hooked": "^4.3.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.1",
        "@types/humps": "^2.0.6",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.8",
        "@types/node": "^22.13.4",
        "@types/pg": "^8.15.4",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/parser": "^8.24.0",
        "dotenv": "^16.4.7",
        "eslint": "^9.21.0",
        "eslint-plugin-jest": "^28.11.0",
        "jest": "^29.7.0",
        "langsmith": "^0.3.87",
        "node-pg-migrate": "^8.0.3",
        "openevals": "^0.1.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.24.0"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^3.0.35",
        "@ai-sdk/google": "^3.0.20",
        "@ai-sdk/openai": "^3.0.25",
        "@apollo/client": "^3.13.1",
        "@langchain/anthropic": "^0.3.24",
        "@langchain/core": "^0.3.63",
        "@langchain/google-genai": "^0.2.15",
        "@langchain/openai": "^0.6.1",
        "@langfuse/otel": "^4.5.1",
        "@langfuse/tracing": "^4.5.1",
        "@opentelemetry/sdk-node": "^0.211.0",
        "ai": "^6.0.68",
        "ajv": "^8.17.1",
        "axios": "^1.9.0",
        "better-auth": "^1.2.9",
        "cls-hooked": "^4.2.2",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "exa-js": "^2.3.0",
        "globals": "^16.0.0",
        "graphile-utils": "^4.14.1",
        "graphql": "^15.10.1",
        "humps": "^2.0.1",
        "ioredis": "^5.6.1",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "langchain": "^0.3.30",
        "path": "^0.12.7",
        "pg": "^8.16.2",
        "postgraphile": "^4.14.1",
